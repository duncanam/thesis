FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    "(p|rho)"
    {
        solver           GAMG;
        smoother	symGaussSeidel;
        tolerance        1e-10;
        relTol           1e-5;
    }

    "(p|rho)Final"
    {
        $p;
        tolerance        1e-10;
        relTol           0.0;
    }

    "(U|h|k|epsilon)"
    {
        solver          PBiCGStab;
        preconditioner  DILU;
        tolerance       1e-10;
        relTol          1e-5;
    }

    "(U|h|k|epsilon)Final"
    {
        $U;
        relTol          0;
    }

    "Yi.*"
    {
        $hFinal;
    }
}

PIMPLE
{
	transonic yes;
    momentumPredictor no;
    nOuterCorrectors  10;
    nCorrectors     3;
    nNonOrthogonalCorrectors 0;
}


