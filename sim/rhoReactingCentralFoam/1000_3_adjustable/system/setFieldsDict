FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
defaultFieldValues
(
    volScalarFieldValue T 300 // [K]
    volScalarFieldValue p 101325 // [Pa]
	volScalarFieldValue H2 0.02851 // Marcantoni mass fraction
	volScalarFieldValue O2 0.226 // Marcantoni mass fraction
	volScalarFieldValue H2O 0 // mass fraction
);

regions
(
    boxToCell
    {
        box (0 0 -0.001) (0.001 0.04 0);
        fieldValues
        (
				volScalarFieldValue T 3000 // [K]
                volScalarFieldValue p 2.0265e6 // [Pa]
				volScalarFieldValue H2 0 // mass fraction
				volScalarFieldValue O2 0 // mass fraction
				volScalarFieldValue H2O 0.255 // mass fraction 
        );
    }
);


